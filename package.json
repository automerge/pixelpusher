{
  "name": "pixelpusher",
  "productName": "pixelpusher",
  "version": "1.0.0",
  "description": "Draw and animate Pixel Art, then export the results to CSS, share or download them, powered by Hypermerge",
  "main": "src/electron.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "css": "postcss --watch --dir=build src/css/imports.css"
  },
  "keywords": [],
  "author": "jeffpeterson",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "pixel_art_forge"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "pixelartforge"
      }
    }
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.2.0",
    "cors": "^2.8.4",
    "css-loader": "^0.28.7",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gm": "^1.23.0",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^3.8.2",
    "ipfs": "^0.27.3",
    "ipfs-pubsub-room": "^1.1.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "lost": "^8.2.0",
    "node-localstorage": "^1.3.0",
    "oauth": "^0.9.15",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-reporter": "^5.0.0",
    "precss": "^2.0.0",
    "pug": "^2.0.0-rc.4",
    "radium": "^0.19.6",
    "react": "^16.1.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-color": "^2.13.8",
    "react-cookie-banner": "0.0.18",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.1.0",
    "react-hot-loader": "^3.1.2",
    "react-modal": "^3.1.2",
    "react-redux": "^5.0.6",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-undo": "^0.6.1",
    "shortid": "^2.2.8",
    "style-loader": "^0.19.0",
    "temp": "^0.8.3",
    "twitter": "^1.7.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.20.0",
    "whatwg-fetch": "^2.0.3"
